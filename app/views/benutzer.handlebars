<div class="row">
  <div class="col">
    <h1>Profil von {{ benutzername }}</h1>
  </div>
</div>

<div class="row">
  <div class="col">
    <h2>Touren von {{ benutzername }}</h2>
    <ul class="list-group">
      {{#each touren}}
      <li class="list-group-item">
        <a href="/touren/{{this.name}}">{{this.name}}</a>
      </li>
      {{/each}}
    </ul>
  </div>
</div>